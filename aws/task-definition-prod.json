{
  "requiresCompatibilities": ["FARGATE"],
  "family": "LeanJenkinsApp-TaskDefinition-Prod",
  "executionRoleArn": "",
  "networkMode": "none",
  "containerDefinitions": [
    {
      "name": "learnjenkins",
      "image": "nginx:1.26-alpine",
      
      "portMappings": [
        {
          "name": "nginx-80",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
           "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [],  
  "cpu": "256",
  "memory": "512"
}
